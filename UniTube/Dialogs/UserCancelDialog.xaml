<ContentDialog
    x:Class="UniTube.Dialogs.UserCancelDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:cu="http://schemas.microsoft.com/winfx/2006/xaml/presentation?IsApiContractNotPresent(Windows.Foundation.UniversalApiContract,5)"
    xmlns:fcu="http://schemas.microsoft.com/winfx/2006/xaml/presentation?IsApiContractPresent(Windows.Foundation.UniversalApiContract,5)"
    mc:Ignorable="d"
    Title="{Binding Source={StaticResource Localized}, Path=[UserCancelDialogTitle]}"
    PrimaryButtonText="{Binding Source={StaticResource Localized}, Path=[CloseDialogButton]}"
    cu:Background="{ThemeResource SystemControlBackgroundChromeMediumBrush}"
    fcu:Background="{ThemeResource SystemControlAcrylicElementBrush}"
    Content="{Binding Source={StaticResource Localized}, Path=[UserCancelDialogMessage]}"/>